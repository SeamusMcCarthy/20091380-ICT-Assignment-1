{{#each playlists}}
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/playlist/{{id}}" class="ui icon button"><i class="folder open icon"></i></a>
    <a href="/deleteplaylist/{{id}}" class="ui icon button"><i class="trash alternate outline icon"></i></a>
{{/each}}